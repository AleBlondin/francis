{
  "name": "francis",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "connected-react-router": "^4.5.0",
    "env-cmd": "^8.0.2",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.24.1",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-mysticatea": "^4.2.4",
    "eslint-plugin-prefer-object-spread": "^1.2.1",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "history": "^4.7.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-intl": "^2.7.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.0.4",
    "react-test-renderer": "^16.5.0",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0",
    "source-map-explorer": "^1.6.0",
    "styled-components": "^3.4.5",
    "superagent": "^4.0.0-beta.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "env-cmd .env.${REACT_APP_ENV} react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "analyze": "source-map-explorer build/static/js/main.*",
    "test:coverage": "npm run test -- --coverage",
    "flow": "flow",
    "flow:coverage": "flow-coverage-report --config=.flowcoverageconfig",
    "generate": "plop --plopfile scripts/generators/index.js",
    "lint": "eslint --ext .jsx,.js -c .eslintrc src",
    "lint:fix": "eslint --fix --ext .jsx,.js -c .eslintrc src"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.test.js",
      "!src/**/index.js",
      "!src/**/registerServiceWorker.js",
      "!src/**/*.container.js",
      "!src/index.js",
      "!src/tempPolyfills.js",
      "!src/setupTests.js",
      "!src/redux/reducers.js",
      "!src/redux/sagas.js",
      "!src/redux/store.js"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 85,
        "branches": 100,
        "functions": 75,
        "lines": 90
      }
    }
  },
  "devDependencies": {
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "enzyme-to-json": "^3.3.4",
    "flow-bin": "^0.82.0",
    "flow-coverage-report": "^0.6.0",
    "flow-typed": "^2.5.1",
    "plop": "^2.1.0",
    "prettier": "^1.14.3"
  }
}
